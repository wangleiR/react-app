{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "latest",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0"
  },
  "scripts": {
    "build-css": "node-sass src/sass -o src/css",
    "watch-css": "npm run build-css && node-sass src/sass -o src/css --watch --recursive",
    "start-f": "react-scripts start",
    "start": "npm-run-all -p watch-css start-f",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "react-app-rewired": "^1.5.0",
    "redux-devtools": "^3.4.1",
    "sass-loader": "^7.0.1"
  }
}
